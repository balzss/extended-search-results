{
    "name":"Extended Search Results",
    "description":"this extension adds useful info to your search results",
    "version":"1",
    "manifest_version":2,
    "key": "",
    "permissions": [
        "https://api.github.com/*",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.google.hu/*"],
            "js": [
                "script.js"
            ]
        }
    ],
    "oauth2": {
        "client_id": "1062166061977-ck220ok2odoh1jl9s1ifj0geupn9v883.apps.googleusercontent.com",
        "scopes":[""]
    },
    "browser_action": {
        "default_title": "Extended Search Results",
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    }
}
